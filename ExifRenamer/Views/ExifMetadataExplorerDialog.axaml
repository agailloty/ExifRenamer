<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="ExifRenamer.Views.ExifMetadataExplorerDialog"
        xmlns:vm="clr-namespace:ExifRenamer.ViewModels;assembly=ExifRenamer"
        x:DataType="vm:MainWindowViewModel"
        WindowStartupLocation="CenterScreen"
        CanResize="False"
        IsTabStop="True"
        ExtendClientAreaToDecorationsHint="True"
        ShowInTaskbar="False"
        Title="ExifMetadataDialog">
    <Border>
        <Grid>
            <StackPanel>
                <TextBlock Text="Exif Metadata" FontSize="20" FontWeight="Bold" />
                <TextBlock Text="The following metadata will be used to rename the files" />

                <Button Content="OK" Command="{Binding OKCommand}" />
            </StackPanel>
        </Grid>
    </Border>
</Window>